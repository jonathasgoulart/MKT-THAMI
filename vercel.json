{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "*.html",
            "use": "@vercel/static"
        },
        {
            "src": "*.css",
            "use": "@vercel/static"
        },
        {
            "src": "*.js",
            "use": "@vercel/static"
        },
        {
            "src": "icons/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "manifest.json",
            "use": "@vercel/static"
        },
        {
            "src": "sw.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/chat",
            "dest": "/api/chat.js"
        }
    ]
}